<IfModule mod_headers.c>
  Header set X-XSS-Protection "1; mode=block"
  Header set X-Content-Type-Options nosniff
  Header always append X-Frame-Options SAMEORIGIN
  Header set Strict-Transport-Security "max-age=31536000"
  #Header always set Content-Security-Policy "frame-ancestors 'self'; default-src 'self'; style-src 'self' fonts.bunny.net 'unsafe-inline' unpkg.com  cdn.jsdelivr.net cdnjs.cloudflare.com fonts.googleapis.com use.fontawesome.com maxcdn.bootstrapcdn.com cdn.datatables.net; font-src 'self' data: viacaotapajos.homologacao.yjaraviagens.com use.fontawesome.com fonts.bunny.net fonts.gstatic.com; script-src 'self' 'unsafe-inline' 'unsafe-eval' www.criarenquete.com.br i.k-analytix.com checkout.safe2pay.com.br unpkg.com cdnjs.cloudflare.com googletagmanager.com www.googletagmanager.com code.jquery.com connect.facebook.net cdn.datatables.net www.google.com cdn.jsdelivr.net viacep.com.br maxcdn.bootstrapcdn.com www.gstatic.com apis.google.com use.fontawesome.com sandbox.boletobancario.com https://www.googletagmanager.com/gtag/js https://www.google-analytics.com; frame-src https://www.googletagmanager.com s1.criarenquete.com.br www.gstatic.com www.google.com; img-src 'self' localhost upload.wikimedia.org cdn-icons-png.flaticon.com www.googletagmanager.com www.google-analytics.com stats.g.doubleclick.net www.google.com www.google.com.br data: www.google-analytics.com blob: https://app.yjaraviagens.com; connect-src 'self' * blob: https://app.yjaraviagens.com"
  Header always set Referrer-Policy "same-origin"
  Header set Feature-Policy "geolocation 'self'; vibrate 'none'"
  Header set Access-Control-Allow-Methods "GET, POST, PUT, DELETE, OPTIONS"
  Header set Cross-Origin-Resource-Policy 'cross-origin'
</IfModule>
